{
  "name": "stalk-studio",
  "version": "0.3.1",
  "description": "Highly flexible trace analysis tool for Jaeger and Zipkin",
  "homepage": "https://github.com/dgurkaynak/stalk-studio",
  "repository": "github:dgurkaynak/stalk-studio",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Deniz Gurkaynak",
    "email": "dgurkaynak@gmail.com",
    "url": "https://github.com/dgurkaynak"
  },
  "main": "main.js",
  "dependencies": {
    "electron-window-state": "^5.0.3",
    "node-int64": "^0.4.0"
  },
  "scripts": {
    "build": "webpack --mode production",
    "commit": "git-cz",
    "format": "prettier --write .",
    "package": "electron-builder --dir",
    "distribute": "electron-builder",
    "start:electron": "node scripts/electron-wait-webpack-dev-server.js",
    "start:dev-server": "webpack-dev-server --port 9000",
    "start": "cross-env BROWSER=none nf start"
  },
  "devDependencies": {
    "@mdi/js": "^5.3.45",
    "@mdi/react": "^1.4.0",
    "@mdi/svg": "^4.5.95",
    "@phosphor/widgets": "^1.9.3",
    "@types/chroma-js": "^1.4.3",
    "@types/events": "^3.0.0",
    "@types/lodash": "^4.14.133",
    "@types/logfmt": "^1.2.1",
    "@types/node": "12.12.47",
    "@types/object-hash": "^1.3.1",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "chance": "^1.1.3",
    "chroma-js": "^2.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "date-fns": "^2.11.0",
    "dexie": "^2.0.4",
    "dotenv": "^8.2.0",
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0",
    "electron-notarize": "^1.0.0",
    "events": "^3.0.0",
    "file-loader": "^6.0.0",
    "flatpickr": "^4.6.3",
    "focus-trap": "^5.1.0",
    "foreman": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^3.1.0",
    "fuse.js": "^3.4.6",
    "google-palette": "^1.1.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-inline-svg-plugin": "^1.2.4",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.19",
    "logfmt": "^1.3.2",
    "monaco-editor": "^0.20.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "noty": "^3.2.0-beta",
    "object-hash": "^2.0.3",
    "opentracing": "^0.14.4",
    "parse-duration": "^0.1.1",
    "prettier": "2.0.5",
    "raw-loader": "^3.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "shortid": "^2.2.15",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "tabulator-tables": "^4.5.3",
    "tippy.js": "^5.1.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2",
    "uglifycss": "0.0.29",
    "url-join": "^4.0.1",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "xstate": "^4.7.0-rc5"
  },
  "build": {
    "productName": "Stalk Studio",
    "appId": "com.denizgurkaynak.stalk-studio",
    "files": [
      "main.js",
      "assets/**/*",
      "build/**/*",
      "!build/**/*.js.map",
      "!build/report.html"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "afterSign": "scripts/electron-notarize.js",
    "mac": {
      "category": "public.app-category.developer-tools",
      "type": "distribution",
      "fileAssociations": [
        {
          "ext": [
            "json",
            "JSON"
          ],
          "description": "JSON files",
          "role": "Viewer"
        }
      ],
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist"
    },
    "linux": {
      "category": "Development"
    },
    "win": {}
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
